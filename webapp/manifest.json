{
	"_version": "1.8.0",
	"sap.app": {
		"id": "qperior.mw_challenge.mw_challenge",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		},
		"dataSources": {
			"MW_Challenge": {
				"uri": "/IKH/q_perior/mw_challenge/services/MW_Challenge.xsodata",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize"
		],
		"fullWidth": true
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "qperior.mw_challenge.mw_challenge.view.Startpage",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.layout": {},
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "qperior.mw_challenge.mw_challenge.i18n.i18n"
				}
			},
			"": {
				"dataSource": "MW_Challenge",
				"settings": {
					"defaultBindingMode": "TwoWay"
				}
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "qperior.mw_challenge.mw_challenge.view",
				"controlAggregation": "pages",
				"controlId": "idAppControl",
				"clearControlAggregation": false,
				"fullWidth": true
			},
			"routes": [
				{
					"name": "Startpage",
					"pattern": "Startpage",
					"target": [
						"Startpage"
					]
				},
				{
					"name": "Checkin",
					"pattern": "Checkin",
					"target": [
						"Checkin"
					]
				}
			],
			"targets": {
				"Startpage": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewName": "Startpage"
				},
				"Checkin": {
					"viewType": "XML",
					"viewName": "Checkin"
				},
				"Base": {
					"viewType": "XML",
					"viewName": "Base"
				},
				"Finish": {
					"viewType": "XML",
					"viewName": "Finish"
				}
			}
		}
	}
}